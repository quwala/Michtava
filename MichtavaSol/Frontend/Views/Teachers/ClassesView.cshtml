
<br /><br /><br />
<head>
    <style>
        h2 {
            font-weight: bold;
            font: 50px arial, sans-serif;
        }

        input[type='submit'] {
            font-size: 32px;
            background-color: darkorange;
            border-color: darkorange;
        }

        body {
            background-image: url("../../Backgrounds/background.jpg");
            width: 100%;
            height: 100%;
            background-position: center;
            background-size: 100% 100%;
            background-repeat: no-repeat;
        }
    </style>
</head>

<body>
    <div class="row">
        <div class="row">
            <div class="col-sm-10">
                <b><h2>@ViewBag.Title:</h2></b>
            </div>
        </div>
        <br /><br />

        @foreach (var cls in TempData.Values)
        {
            <div class="row">
                <div class="col-sm-10">
                    <input type="submit" value="@cls"/>
                </div>
            </div>
        }

    </div>
    <br /><br /><br /><br /><br /><br /><br />
</body>

