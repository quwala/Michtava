

<br /><br /><br />
<head>
    <style>
        h2 {
            font-weight: bold;
            font: 50px arial, sans-serif;
        }

        input[type='submit'] {
            font-size: 32px;
            background-color: darkorange;
            border-color: darkorange;
        }

        body {
            background-image: url("../../Backgrounds/background.jpg");
            width: 100%;
            height: 100%;
            background-position: center;
            background-size: 100% 100%;
            background-repeat: no-repeat;
        }
    </style>
</head>

<body>
<div class="row">
    <div class="row">
        <div class="col-sm-10">
            <b>
                <h2>@ViewBag.Title</h2></b>
        </div>
    </div>
    <br/>
    <div class="row">
        <h4>@TempData["email"]</h4>
    </div>
    <br/>
    <div class="row">
        <h4>@TempData["phoneNumber"]</h4>
    </div>
    <br/>
    <div class="row">
        <h4>@TempData["class"]</h4>
    </div>
    <br/>
    <div class="row">
        <h4>@TempData["userName"]</h4>
    </div>
    <br/>

    @foreach (var data in TempData)
    {
        if (data.Key.Contains("homework"))
        {
            <div class="row">
                <h4>@data.Value</h4>
            </div>
        }
    }

</div>
</body>

